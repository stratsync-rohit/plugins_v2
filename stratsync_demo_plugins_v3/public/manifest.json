{
  "manifest_version": 3,
  "name": "Rohit Choukiker",
  "version": "2.0.0",
  "description": "StratSync Demo Plugin V3",

  "permissions": [
    "windows",
    "tabs",
    "scripting",
    "activeTab"
  ],

  "host_permissions": ["<all_urls>"],

  "background": {
    "service_worker": "background.js"
  },

  "action": {
    "default_popup": "index.html",
    "default_title": "Open Demo StratSync V3",
    "default_icon": {
      "16": "logo.jpeg",
      "48": "logo.jpeg",
      "128": "logo.jpeg"
    }
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],

  "web_accessible_resources": [
    {
      "resources": ["logo.jpeg"],
      "matches": ["<all_urls>"]
    }
  ],

  "icons": {
    "16": "logo.jpeg",
    "48": "logo.jpeg",
    "128": "logo.jpeg"
  }
}
