{
  "manifest_version": 3,
  "name": "Rohit Choukiker",
  "version": "1.0.1",
  "description": "StratSync Demo Plugin",
  "permissions": ["windows", "tabs"],

  "action": {
    "default_popup": "index.html",
    "default_title": "Open Demo StratSync"
  },

  "background": {
    "service_worker": "background.js"
  },

  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ]

}
